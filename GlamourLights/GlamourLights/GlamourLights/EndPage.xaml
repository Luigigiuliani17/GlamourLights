<Page x:Class="GlamourLights.EndPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GlamourLights"
      mc:Ignorable="d" 
      d:DesignHeight="520" d:DesignWidth="720"
      VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
      Title="EndPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock x:Name="endBlock" Style="{StaticResource textBlockStyle}" Grid.Row="0" FontSize="35">
        </TextBlock>
        <Button x:Name="startButton" Style="{StaticResource buttonStyle}" Grid.Row="1" FontSize="25" Click="startButton_Click">
            Start
        </Button>
    </Grid>
</Page>
